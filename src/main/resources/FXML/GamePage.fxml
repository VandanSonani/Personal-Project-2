<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<Pane prefHeight="850" prefWidth="900" styleClass="pane" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MyController">
	<Label layoutX="8.0" layoutY="56.0" text="Wager:" />
	<TextField layoutX="8.0" layoutY="72.0" />
	<Label layoutX="8.0" layoutY="156.0" text="Pair Plus:" />
	<TextField fx:id="PairPlus" layoutX="8.0" layoutY="172" />
	<Label layoutX="8.0" layoutY="256.0" text="Ante:" />
	<TextField fx:id="Ante" layoutX="8.0" layoutY="272" />
	<Label layoutX="8.0" layoutY="356.0" text="Total Winnings:" />
	<TextField disable="true" layoutX="8.0" layoutY="372" />

	<Label layoutX="733.0" layoutY="56.0" text="Wager:" />
	<TextField layoutX="733.0" layoutY="72.0" />
	<Label layoutX="733.0" layoutY="156.0" text="Pair Plus:" />
	<TextField fx:id="PairPlus2" layoutX="733.0" layoutY="172" />
	<Label layoutX="733.0" layoutY="256.0" text="Ante:" />
	<TextField fx:id="Ante2" layoutX="733.0" layoutY="272" />
	<Label layoutX="733.0" layoutY="356.0" text="Total Winnings:" />
	<TextField disable="true" layoutX="733.0" layoutY="372" />

	<MenuBar prefHeight="30.0" prefWidth="81.0" styleClass="menubar">
		<menus>
			<Menu mnemonicParsing="false" text="Options">
				<items>
					<MenuItem mnemonicParsing="false" onAction="#exitApplication" text="Exit" />
				</items>
				<items>
					<MenuItem mnemonicParsing="false" text="Fresh Start" />
				</items>
				<items>
					<MenuItem mnemonicParsing="false" text="NewLook" />
				</items>
			</Menu>
		</menus>
	</MenuBar>

	<Button fx:id="foldPlayer1Button" layoutX="370.0" layoutY="800.0" mnemonicParsing="false" onAction="#foldPlayer1" text="Player1 Fold" />
	<Button fx:id="foldPlayer2Button" layoutX="490" layoutY="800" mnemonicParsing="false" onAction="#foldPlayer2" text="Player2 Fold" />
	<Button layoutX="449.0" layoutY="723.0" mnemonicParsing="false" onAction="#dealCardsFunc" text="Deal" />
	<Button layoutX="450.0" layoutY="762.0" mnemonicParsing="false" onAction="#playCards" text="Play" />

	<HBox fx:id="player1Hand1" layoutY="450" prefHeight="250" prefWidth="450" spacing="10" alignment="CENTER" />
	<HBox fx:id="player2Hand2" layoutX="478.0" layoutY="450" prefHeight="250" prefWidth="422.0" spacing="10" alignment="CENTER" />
	<HBox fx:id="dealerHand1" layoutX="250" layoutY="100" prefHeight="250" prefWidth="400" spacing="10" alignment="CENTER" />
	<Label layoutX="427.0" layoutY="45.0" text="Dealer">
		<font>
			<Font size="32.0" />
		</font>
	</Label>
	<Label layoutX="152.0" layoutY="728.0" text="Player 1" />
	<Label layoutX="746.0" layoutY="728.0" text="Player 2" />
</Pane>
